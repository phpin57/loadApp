<Chargement>
	BoxLayout:
		orientation:"vertical"

		Label:
			font_size: 15
			text: "Ajout d'une charge"
		BoxLayout:
			orientation:"horizontal"
			Label:
				font_size: 15
				text: 'Poids (kg)'
			TextInput:
				id: poids
				multiline: False
		BoxLayout:
			orientation:"horizontal"
			Label:
				font_size: 15
				text: 'H-armes (m)'
			TextInput:
				id: h_armes
				multiline: False
		BoxLayout:
			orientation:"horizontal"
			Label:
				font_size: 15
				text: 'longueur (m)'
			TextInput:
				id: longueur
				multiline: False

		BoxLayout:
			orientation:"horizontal"
			Label:
				font_size: 15
				text: 'larguage'
			Spinner: 
				id: largage_mat
				text: "non"
				values: ["non","1","2","3"]

		BoxLayout:
			size_hint_y: 0.5
			orientation:"horizontal"
			Button:
				font_size: 15
				text: "supprimer le dernier"
				on_press: root.supprimer()
			Button:
				font_size: 15
				text: "ajout"
				on_press: root.ajout(poids.text, h_armes.text, longueur.text,largage_mat.text)


		ScrollView:
			do_scroll_x: False
			do_scroll_y: True
			size_hint_y: 6

			Label:
				id: data_value
				size_hint_y: None
				height: self.texture_size[1]
				text_size: self.width, None
				font_size: 20
				padding: 10, 10
				text:""

