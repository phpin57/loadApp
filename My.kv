<Setup>:
	BoxLayout:
		orientation:"vertical"
		Label:
			font_size: 30
			text: 'Données à saisir'
		BoxLayout:
			orientation:"horizontal"
			Label:
				font_size: 15
				text: 'APM (kg)'
			TextInput:
				id: APM
				multiline: False
				text:'80000'
			Label:
				font_size: 15
				text: 'APM CG (%)'
			TextInput:
				id: APMCG
				multiline: False
				text:'30'
		BoxLayout:
			orientation:"horizontal"
			Label:
				font_size: 15
				text: 'fuel (kg)'
			TextInput:
				id: fuel
				multiline: False
				text:'10000'
			Label:
				font_size: 15
				text: 'conso (kg)'
			TextInput:
				id: fuel_conso
				multiline: False
		BoxLayout:
			orientation:"horizontal"
			Label:
				font_size: 15
				text: 'mode de vol'
			Spinner: 
				id: spinner_vol
				text: "LH"
				values: ["LH", "TLL1"]
		BoxLayout:
			orientation:"horizontal"
			Label:
				font_size: 15
				text: 'Version siège'
			Spinner: 
				id: version_lat
				text: "aucune"
				values: ["aucune", "version log", "6*25", "6*29"]
			Label:
				font_size: 15
				text: 'nombre de bloc au centre'
			Spinner: 
				id: nb_bloc
				text: "0"
				values: ["0","1","2","3","4","5","6"]
		BoxLayout:
			orientation:"horizontal"
			Label:
				text: 'Indication des shearing & bending'
				font_size: 15
			CheckBox:
				on_active: root.shearing_and_bending()
				id : chk_shear
			Button:
				font_size: 15
				text: "Shearing\n And Bending"
				on_press: root.shearing_and_bending()
		BoxLayout:
			orientation:"horizontal"
			Label:
				font_size: 15
				text: 'masse équipement (kg)'
			TextInput:
				id: equipement
				multiline: False
				text:"500"
		Label:
			font_size: 30
			text: 'Items classe II'
		BoxLayout:
			orientation:"horizontal"
			Label:
				text: 'Blindage'
				text_size: self.size
			CheckBox:
				id : chk_blindage
		BoxLayout:
			orientation:"horizontal"
			Label:
				text: 'Full medevac'
				text_size: self.size
			CheckBox:
				id : chk_medevac
		BoxLayout:
			orientation:"horizontal"
			Label:
				text: 'HDU'
				text_size: self.size
			CheckBox:
				id : chk_hdu
		BoxLayout:
			orientation:"horizontal"
			Label:
				text: 'Crane'
				text_size: self.size
			CheckBox:
				id : chk_crane

		Button:
			font_size: 15
			text: 'Valider'
			on_press:root.valider()

<ShearingAndBending>
	BoxLayout:
		orientation:"vertical"
		BoxLayout:
			size_hint_y: 0.5
