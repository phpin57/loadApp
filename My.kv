			Label:
				font_size: 15
				text: 'Shearing limit'
			Label:
				id:shear_lim
				font_size: 15
				text: ''
			Label:
				font_size: 15
				text: 'Bending limit'
			Label:
				id:bending_lim
				font_size: 15
				text: ''


		BoxLayout:
			size_hint_y: 0.2
			orientation:"horizontal"
			Button:
				font_size: 15
