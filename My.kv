<NomPopup>
	BoxLayout:
		orientation:"vertical"
		size_hint: 1, 1
		pos_hint: {"x":0, "top":1}
		

		Label:
			font_size: 15
			text: "Noms sauvegarde :"
		TextInput:
			id: nom
			size_hint_y: 0.3
			multiline: False


		BoxLayout:
			orientation:"horizontal"
			size_hint_y: 0.2
			Button:
				font_size: 15
				text: "Annuler"
				on_press: root.popup.dismiss()
			Button:
				font_size: 15
				text: "Valider"
				on_press: root.valider(nom.text)
					


